  "scripts": {
    "build": "webpack-cli --config webpack.production.ts",
    "build:all": "npm run build",
    "lint": "npm run lint:base",
    "lint:base": "eslint \"src/**/*.{js,ts}{,?}\"",
    "lint:fix": "npm run lint:base -f table --fix",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "start": "webpack-dev-server --config webpack.development.ts --host 0.0.0.0 --hot",
    "test": "jest --runInBand --changedSince=develop",
    "test:clearCache": "jest --clearCache",
    "test:coverage": "jest --runInBand --collectCoverage",
    "test:snap": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tsc && npm run test"
